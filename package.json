{
  "name": "nip-sip",
  "version": "2.0.0",
  "description": "Nip Sip, a place to buy nips",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "seed": "node script/seed.js",
    "start": "node server",
    "start:dev": "npm run build:dev & npm run start-server",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start-server": "JWT=shh nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV=test JWT=a_secret mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" --require @babel/register --require esm ",
    "test:dev": "npm run test -- --watch",
    "test:dev:models": "npm run test:dev -- --grep=model",
    "test:dev:routes": "npm run test:dev -- --grep=route",
    "test:jest": "jest --watchAll --testPathIgnorePatterns=server/db",
    "test:db": "NODE_ENV=test JWT=a_secret jest --watchAll --runInBand --verbose --testPathPattern=server/db/models"
  },
  "author": "Adam, Kyle, Jason, Sey",
  "license": "MIT",
  "dependencies": {
    "@babel/preset-env": "^7.14.5",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.1",
    "babel-jest": "^27.0.2",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "framer-motion": "^4.1.17",
    "fuse.js": "^6.4.6",
    "history": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "react-responsive-carousel": "^3.2.18",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.16.0",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.1",
    "css-loader": "^5.2.6",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "esm": "^3.2.25",
    "jest": "^27.0.4",
    "mocha": "^6.2.3",
    "nodemon": "^1.19.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.1",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "style-loader": "^2.0.0",
    "supertest": "^4.0.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1"
  }
}
